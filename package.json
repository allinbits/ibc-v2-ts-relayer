{
  "name": "ibc-v2-ts-relayer",
  "version": "0.0.1",
  "description": "A relayer in TS for IBC v2",
  "scripts": {
    "build": "tsc --noEmit && tsdown",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:gno:e2e": "vitest run --config vitest.config.gno.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "bin": {
    "ibc-v2-relayer": "./dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@atomone/cosmos-ibc-types": "^1.0.4",
    "@clockworkgr/ibc-v2-client-ts": "^0.0.1",
    "@cosmjs/crypto": "^0.37.0",
    "@cosmjs/encoding": "^0.36.2",
    "@cosmjs/math": "^0.36.2",
    "@cosmjs/proto-signing": "^0.36.2",
    "@cosmjs/stargate": "^0.36.2",
    "@cosmjs/tendermint-rpc": "^0.36.2",
    "@cosmjs/utils": "^0.36.2",
    "@gnolang/gno-js-client": "^1.4.5",
    "@gnolang/gno-types": "^1.0.8",
    "@gnolang/tm2-js-client": "^1.3.3",
    "@gnolang/tm2-rpc": "^0.0.9",
    "@napi-rs/keyring": "^1.2.0",
    "better-sqlite3": "^12.6.2",
    "commander": "^14.0.3",
    "cosmjs-types": "^0.10.1",
    "dexie": "^4.3.0",
    "fake-indexeddb": "^6.2.5",
    "graphql-request": "^7.4.0",
    "handlebars": "^4.7.8",
    "long": "^5.3.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "wait-on": "^9.0.4",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@eslint/js": "^10.0.1",
    "@keplr-wallet/types": "^0.13.10",
    "@stylistic/eslint-plugin": "^5.9.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.3.0",
    "@vitest/coverage-istanbul": "^4.0.18",
    "eslint": "^10.0.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "happy-dom": "^20.7.0",
    "jsdom": "^28.1.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts"
}
