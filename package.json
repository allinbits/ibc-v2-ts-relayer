{
  "name": "ibc-v2-ts-relayer",
  "version": "0.0.1",
  "description": "A relayer in TS for IBC v2",
  "scripts": {
    "build": "tsc --noEmit && tsdown",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "bin": {
    "ibc-v2-relayer": "./dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@atomone/cosmos-ibc-types": "^1.0.3",
    "@clockworkgr/ibc-v2-client-ts": "^0.0.1",
    "@cosmjs/encoding": "^0.36.0",
    "@cosmjs/math": "^0.36.0",
    "@cosmjs/proto-signing": "^0.36.0",
    "@cosmjs/stargate": "^0.36.0",
    "@cosmjs/tendermint-rpc": "^0.36.0",
    "@cosmjs/utils": "^0.36.0",
    "@gnolang/tm2-rpc": "^0.0.4",
    "@napi-rs/keyring": "^1.2.0",
    "better-sqlite3": "^12.2.0",
    "commander": "^14.0.1",
    "cosmjs-types": "^0.10.1",
    "dexie": "^4.2.0",
    "fake-indexeddb": "^6.2.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "wait-on": "^9.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@eslint/js": "^9.35.0",
    "@keplr-wallet/types": "^0.12.273",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.5.2",
    "@vitest/coverage-istanbul": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "happy-dom": "^18.0.1",
    "jsdom": "^27.0.0",
    "tsdown": "^0.15.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vitest": "^3.2.4"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  }
}
